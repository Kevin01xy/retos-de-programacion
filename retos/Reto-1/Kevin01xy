import java.util.Scanner;

public class Fibonacci {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Ingresa la cantidad de n√∫meros de la secuencia Fibonacci a mostrar: ");
        int n = sc.nextInt();
        int inicialNumber = 0;
        int secondNumber = 1;
        int thirdNumber;

        for(int i =1;i<=n;i++){
            thirdNumber = inicialNumber;
            inicialNumber = inicialNumber + secondNumber;
            secondNumber = thirdNumber;
            System.out.println(secondNumber);
        }
    }
}
